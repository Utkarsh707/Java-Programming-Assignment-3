import java.util.Scanner;
public class Main
{
  public static void main (String[]args)
  {
    Scanner s = new Scanner (System.in);
      System.out.println ("Enter a character:");

    int ascii_val = s.next ().charAt (0);
    //System.out.println(ascii_val);
    if ((ascii_val >= 65 && ascii_val <= 90) || (ascii_val >= 97)
	&& ascii_val <= 122)
      {
	System.out.print ((char) ascii_val + " is an alphabet");
	if ((char) ascii_val == 'a' || (char) ascii_val == 'e'
	    || (char) ascii_val == 'i' || (char) ascii_val == 'o'
	    || (char) ascii_val == 'u' || (char) ascii_val == 'A'
	    || (char) ascii_val == 'E' || (char) ascii_val == 'I'
	    || (char) ascii_val == 'O' || (char) ascii_val == 'U')
	  {
	    System.out.println (" and a vowel");
	  }
	else
	  {
	    System.out.println (" but not a vowel");
	  }
      }
    else
      {
	System.out.println (ascii_val + " is not an alphabet");
      }
  }
}

